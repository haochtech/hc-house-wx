@import "../../resource/wxParse/wxParse.wxss";

.container {
    background-color: #FFF;
    min-height: 100%;
    display: flex;
    flex-direction: column;
}

.container .title {
    display: flex;
    flex-direction: column;
    width: 750rpx;
    border-bottom: 1px dotted #ccc;
}

.container .title view {
}

.container .title view:nth-child(1) {
    font-size: .9rem;
    font-weight: bold;
    padding-left: 10rpx;
    padding-top: 10rpx;
    padding-bottom: 10rpx;
}

.container .title view:nth-child(2) {
    font-size: .6rem;
    padding-left: 10rpx;
    padding-bottom: 10rpx;
}

.container .content {
    align-self: center;
}