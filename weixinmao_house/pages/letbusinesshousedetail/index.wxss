.type-container {
    width: 100%;
    background-color: #fff;
    overflow: hidden;
}

.type-navbar {
    white-space: nowrap;
    background-color: #fff;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.type-box {
    padding-top: 10rpx;
    width: 250rpx;
    box-sizing: content-box;
    font-size: 24rpx;
    text-align: center;
    display: inline-block;
    overflow: hidden;
}

.type-box image {
    width: 230rpx;
    height: 230rpx;
}

.type-navbar-item {
    border-bottom: 4rpx solid #fff;
}

.type-item-on {
    border-bottom: 4rpx solid #e64340;
}

