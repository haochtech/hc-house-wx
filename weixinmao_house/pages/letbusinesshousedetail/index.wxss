.container {
    background-color: #e5e5e5;
    min-height: 100%;
}

.swiper-container {
    width: 750rpx;
    position: relative;
}

.swiper_box {
    width: 100%;
}

swiper-item image {
    width: 100%;
    display: inline-block;
    overflow: hidden;
}

.swiper-container .dots {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 20rpx;
    display: flex;
    justify-content: center;
}

.swiper-container .dots .dot {
    margin: 0 8rpx;
    width: 14rpx;
    height: 14rpx;
    background: #fff;
    border-radius: 50%;
    transition: all .6s;
    opacity: .5;
}

.swiper-container .dots .dot.active {
    width: 14rpx;
    opacity: 1;
}

.container .header {
    display: flex;
    width: 750rpx;
    flex-direction: column;
    align-items: center;
    background: #FFF;
}

.container .header>image {
    width: 750rpx;
    height: 350rpx;
}

.container .main {
    display: flex;
    width: 750rpx;
    flex-direction: column;
    padding-bottom: 20rpx;
    background: #FFF;
}

.container .main .title {
    display: flex;
    width: 700rpx;
    flex-direction: column;
    padding-top: 20rpx;
    font-size: .9rem;
    padding-left: 30rpx;
}

.container .main .title view {
}

.container .main .title view text {
    color: #1aac19;
}

.container .main .title view:nth-child(1) {
    font-size: 1.1rem;
}

.container .main .title .price {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.container .main .title .price view:nth-child(1) {
    font-size: .9rem;
    color: #d51a39;
}

.container .main .title .price view:nth-child(2) {
    font-size: .7rem;
    color: #FFF;
    background: #ff706a;
    border-radius: 4rpx;
    padding: 5rpx 20rpx;
    margin-left: 100rpx;
}

.container .main .title .price view:nth-child(3) {
    font-size: .7rem;
    color: #FFF;
    background: #ff706a;
    border-radius: 4rpx;
    padding: 5rpx 20rpx;
    margin-left: 50rpx;
}

.container .main .address {
    display: flex;
    flex-direction: column;
    padding-left: 30rpx;
    font-size: .7rem;
    margin-top: 10rpx;
    color: #777676;
}

.container .main .special {
    width: 720rpx;
    display: flex;
    flex-direction: row;
    padding-left: 30rpx;
    margin-top: 20rpx;
    flex-wrap: wrap;
}

.container .listitem .special {
    width: 720rpx;
    display: flex;
    flex-direction: row;
    padding-left: 30rpx;
    padding-bottom: 10rpx;
    margin-top: 20rpx;
    flex-wrap: wrap;
}

.container .listitem .special text {
    padding: 5rpx 10rpx;
    border-radius: 5rpx;
    font-size: .6rem;
    background: red;
    margin-right: 10rpx;
    margin-top: 5rpx;
    color: #FFF;
}

.container .listitem .special text:nth-child(2n) {
    background: #ecf2fe;
    color: #90a1cf;
}

.container .listitem .special text:nth-child(2n-1) {
    background: #e3f9e4;
    color: #7dac82;
}

.container .main .special text {
    padding: 5rpx 10rpx;
    border-radius: 5rpx;
    font-size: .6rem;
    background: red;
    margin-right: 10rpx;
    margin-top: 5rpx;
    color: #FFF;
}

.container .main .special text:nth-child(2n) {
    background: #ecf2fe;
    color: #90a1cf;
}

.container .main .special text:nth-child(2n-1) {
    background: #e3f9e4;
    color: #7dac82;
}

.container .contentlist {
    margin-top: 10rpx;
    width: 750rpx;
    background: #FFF;
    display: flex;
    flex-direction: column;
}

.container .contentlist .title {
    width: 750rpx;
    height: 80rpx;
    line-height: 80rpx;
    font-size: .9rem;
    border-bottom: 1rpx solid #efefef;
    display: flex;
    flex-direction: row;
    align-content: center;
}

.container .contentlist .title view:nth-child(1) {
    height: 40rpx;
    line-height: 40rpx;
    width: 6rpx;
    background: #3274e5;
    align-self: center;
}

.container .contentlist .title view:nth-child(2) {
    height: 80rpx;
    line-height: 80rpx;
    width: 200rpx;
    padding-left: 15rpx;
}

.container .contentlist .listitem {
    width: 750rpx;
    display: flex;
    flex-direction: column;
}

.container .contentlist .listitem .items {
    padding-left: 30rpx;
    color: #777676;
    height: 60rpx;
    line-height: 60rpx;
    font-size: .8rem;
    border-bottom: 1rpx dotted #e5e5e5;
}

.container .contentlist .listitem .items text {
    color: #000;
    padding-left: 10rpx;
}

.agentinfo {
    width: 750rpx;
    display: flex;
    flex-direction: row;
    background: #FFF;
    margin-top: 10rpx;
    justify-content: space-between;
}

.agentinfo .left {
