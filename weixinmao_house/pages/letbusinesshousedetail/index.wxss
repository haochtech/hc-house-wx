}

.agentinfo .left .headpic image {
    width: 120rpx;
    height: 120rpx;
    border: 7rpx solid #FFF;
    border-radius: 120rpx;
}

.agentinfo .left .username {
    display: flex;
    flex-direction: column;
    padding-top: 25rpx;
