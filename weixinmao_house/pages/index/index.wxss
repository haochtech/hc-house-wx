.container {
    background-color: #FFF;
    min-height: 100%;
}

.header {
    width: 750rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    padding-top: 40rpx;
    padding-bottom: 40rpx;
}

.header .city {
    height: 80rpx;
    line-height: 80rpx;
    color: #FFF;
    font-size: .8rem;
}

.header .search {
    width: 600rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 5rpx;
    background: #FFF;
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.header .search view {
    color: #a6a6a6;
    font-size: .7rem;
    align-self: center;
}

.header .search icon {
    margin-right: 30rpx;
    margin-top: 18rpx;
}

.swiper-container {
    width: 750rpx;
    position: relative;
}

.swiper_box {
    width: 100%;
    height: 362.5rpx;
}

swiper-item image {
    width: 100%;
    display: inline-block;
    overflow: hidden;
    height: 362.5rpx;
}

.swiper-container .dots {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 20rpx;
    display: flex;
    justify-content: center;
}

.swiper-container .dots .dot {
    margin: 0 8rpx;
    width: 14rpx;
    height: 14rpx;
    background: #fff;
    border-radius: 50%;
    transition: all .6s;
    opacity: .5;
}

.swiper-container .dots .dot.active {
    width: 14rpx;
    opacity: 1;
}

.navlist {
    width: 750rpx;
    display: flex;
    justify-content: space-around;
    flex-direction: row;
    padding-top: 20rpx;
    flex-wrap: wrap;
}

.shares {
    width: 120rpx;
    height: 120rpx;
    background: #fff;
}

button::after {
    border: 0;
}

.navlist .item {
    width: 180rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: .7rem;
    padding-bottom: 30rpx;
}

.navlist .item image {
    width: 120rpx;
    height: 120rpx;
}

.news {
