}

.news .newslist .title .price view text {
    padding: 10rpx 15rpx;
    border-radius: 3rpx;
    margin-right: 5rpx;
    height: 20rpx;
    line-height: 20rpx;
    color: #77af6e;
    background: #f7f5f6;
}

.news .newslist .title .price .money {
    color: #d65408;
    font-weight: bold;
    font-size: 1rem;
    padding-right: 10rpx;
}

