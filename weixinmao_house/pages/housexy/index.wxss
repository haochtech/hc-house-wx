@import "../../resource/wxParse/wxParse.wxss";

.container {
    background-color: #FFF;
    min-height: 100%;
    display: flex;
    flex-direction: column;
}

.container .title {
    display: flex;
    flex-direction: column;
    width: 750rpx;
    border-bottom: 1px dotted #ccc;
    align-items: center;
}

.container .title view {
}

.container .title view:nth-child(1) {
    font-size: .9rem;
    font-weight: bold;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}

.container .content {
    align-self: center;
    padding-left: 2%;
    padding-right: 2%;
}